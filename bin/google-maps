#!/Users/macbookpro/.rbenv/versions/2.1.0/bin/ruby

require 'uri'

q = ARGV.join(' ')
encoded_q = URI.encode(q)

`open "http://maps.google.com/maps?q=#{encoded_q}"`

__END__

